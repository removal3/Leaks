	LeftHip.C0 = Clerp.LC(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
end
			for i = 0, 0.4, 0.1 / Animation_Speed do
				if taunt.Value ~= "Fave" then
					break
				end
	Swait()
