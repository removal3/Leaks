function RandomCaps(str)
	local new = ""
	for i = 1, #str do
		if(math.random(1,2) == 1)then
			new = new .. (str:sub(i,i):upper())
		else
			new = new .. str:sub(i,i)
		end
	end
	return new
end



if lplr.Name == USERNAME then
game:GetService("UserInputService").InputBegan:Connect(function(k)
	local k = k.KeyCode
		if k == Enum.KeyCode.Return then
			if chatterbro.Text == "Press here (improved chat) for Yeah Universal Speaker" then
				return
			end
			if string.lower(string.sub(chatterbro.Text,1,2)) == "/e" then
				aimlord:FireServer("SpeakerForComm",chatterbro.Text)
				chatterbro.Text = "Press here (improved chat) for Yeah Universal Speaker"
				return
			end
				ChatRemote:FireServer(chatterbro.Text)
				aimlord:FireServer("speaker",chatterbro.Text)
			chatterbro.Text = "Press here (improved chat) for Yeah Universal Speaker"
	end
	end)
	coroutine.resume(coroutine.create(function()
		while not stopeverything do
			Swait()
			if string.len(chatterbro.Text) >= 58 then
				chatterbrohelper.Size = UDim2.new(1,-14,1,-14)
				chatterbrohelper.Position = UDim2.new(0,7,0,7)
			else
				chatterbrohelper.Size = UDim2.new(1,0,0.25,0)
				chatterbrohelper.Position = UDim2.new(0,0,0.75,0)
			end
		end
	end))
	end

function SHAkErCa1(Length,Intensity)
	coroutine.resume(coroutine.create(function()
		local intensity = 1 * Intensity
		local rotM = 0.01 * Intensity
		for i = 0, Length, 0.1 do
			Swait()
			intensity = intensity - 0.05 * Intensity / Length
			rotM = rotM - 5.0E-4 * Intensity / Length
			Humanoid.CameraOffset = Vector3.new(RAD(MRANDOM(-intensity, intensity)), RAD(MRANDOM(-intensity, intensity)), RAD(MRANDOM(-intensity, intensity)))
			cam.CFrame = cam.CFrame * CF(RAD(MRANDOM(-intensity, intensity)), RAD(MRANDOM(-intensity, intensity)), RAD(MRANDOM(-intensity, intensity))) * EULER(RAD(MRANDOM(-intensity, intensity)) * rotM, RAD(MRANDOM(-intensity, intensity)) * rotM, RAD(MRANDOM(-intensity, intensity)) * rotM)
		end
		Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end))
end








