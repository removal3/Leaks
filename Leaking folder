	Neck.C0 = Clerp.LC(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(10), RAD(0), RAD(0)), 1 / Animation_Speed)
	LeftShoulder.C0 = Clerp.LC(LeftShoulder.C0, CF(-1.5, 0.35, 0) * ANGLES(RAD(0), RAD(0), RAD(-15)) * LEFTSHOULDERC0, 1 / Animation_Speed)
	RightHip.C0 = Clerp.LC(RightHip.C0, CF(1, -1.1, -0.3) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
	LeftHip.C0 = Clerp.LC(LeftHip.C0, CF(-1, -1.1, -0.01) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
end
			for i = 0, 0.4, 0.1 / Animation_Speed do
				if taunt.Value ~= "Fave" then
					break
				end
	Swait()
	RootJoint.C0 = Clerp.LC(RootJoint.C0, ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
	Neck.C0 = Clerp.LC(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
