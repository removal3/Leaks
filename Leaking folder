			elseif  taunt.Value == "TG:re2" then 
				GUN.NeonParts.Color = Color3.fromRGB(0.5*eee.PlaybackLoudness,0.5*eee.PlaybackLoudness,0.5*eee.PlaybackLoudness)
				GUN["ùöïùöíùöïùöíùöé"].Color = GUN.NeonParts.Color
				GUN.NeonParts.Transparency = 1
				GUN.Base.Transparency = 1
				GUN.GunAdditions.Transparency = 1
				GUN["ùöïùöíùöïùöíùöé"].Transparency = 1

				Gunny["ùô∑ùöíùöêùöëùöïùöíùöêùöëùöùùöú"].Transparency = 1
				Gunny.gun.Transparency = 1
				Gunny["ùô∑ùöíùöêùöëùöïùöíùöêùöëùöùùöú"].Transparency = 1
			elseif taunt.Value == "demons..." then 
				GUN.NeonParts.Color = Color3.fromRGB(1+150*eee.PlaybackLoudness/350, 0, 1+150*eee.PlaybackLoudness/350)
				GUN["ùöïùöíùöïùöíùöé"].Color = GUN.NeonParts.Color
				GUN.NeonParts.Transparency = 1
				GUN.Base.Transparency = 1
				GUN.GunAdditions.Transparency = 1
				GUN["ùöïùöíùöïùöíùöé"].Transparency = 1

